<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">
	<import resource="classpath:META-INF/cxf/cxf.xml" />
	<import resource="classpath:META-INF/cxf/cxf-extension-soap.xml" />
	<import resource="classpath:META-INF/cxf/cxf-servlet.xml" />
	<jaxws:endpoint
		xmlns:tns="http://mng.tsk.org/taskManagement/UserManagementService/"
		id="usermanagementserviceporttype"
		implementor="org.tsk.mng.taskmanagement.usermanagementservice.UserManagementServicePortTypeImpl"
		wsdlLocation="WEB-INF/wsdl/UserManagementServiceWSDL/UserManagementService.wsdl"
		endpointName="tns:UserManagementServicePort" serviceName="tns:UserManagementService"
		address="/UserManagementServicePort">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
	<jaxws:endpoint
		xmlns:tns="http://mng.tsk.org/taskManagement/TaskManagementService/"
		id="taskmanagementserviceporttype"
		implementor="org.tsk.mng.taskmanagement.taskmanagementservice.TaskManagementServicePortTypeImpl"
		wsdlLocation="WEB-INF/wsdl/TaskManagementServiceWSDL/TaskManagementService.wsdl"
		endpointName="tns:TaskManagementServicePort" serviceName="tns:TaskManagementService"
		address="/TaskManagementServicePort">
		<jaxws:features>
			<bean class="org.apache.cxf.feature.LoggingFeature" />
		</jaxws:features>
	</jaxws:endpoint>
</beans>
